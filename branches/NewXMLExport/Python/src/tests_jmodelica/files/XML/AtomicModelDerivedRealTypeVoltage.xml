<?xml version="1.0" encoding="UTF-8"?>
<class kind="model">
	<classDefinition name="Voltage">
		<builtin name="Real"/>
		<modifier>
			<item name="quantity">
				<string value="ElectricalPotential"/>
			</item>
			<item name="unit">
				<string value="V"/>
			</item>
		</modifier>
	</classDefinition>
	<component name="v">
		<local name="Voltage"/>
		<modifier>
			<item name="start">
				<real value="1.9"/>
			</item>
		</modifier>
	</component>

	<equation kind="initial">
		<equal>
			<local name="v"/>
			<real value="1.9"/>
		</equal>
	</equation>

	<equation>
		<equal>
			<operator name="der">
				<local name="v"/>
			</operator>
			<call builtin="-">
				<call builtin="sin">
					<local name="v"/>
				</call>
			</call>
		</equal>
	</equation>

</class>
