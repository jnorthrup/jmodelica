<?xml version="1.0" encoding="UTF-8"?>
<class kind="model">
	<component name="x">
		<builtin name="Real"/>
	</component>
	<component name="y">
		<builtin name="Real"/>
		<modifier>
			<item name="start">
				<integer value="2"/>
			</item>
		</modifier>
	</component>

	<equation kind="initial">
		<equal>
			<local name="x"/>
			<real value="0.0"/>
		</equal>
		<equal>
			<local name="y"/>
			<integer value="2"/>
		</equal>
	</equation>

	<equation>
		<equal>
			<operator name="der">
				<local name="x"/>
			</operator>
			<builtin name="time"/>
		</equal>
		<equal>
			<operator name="der">
				<local name="y"/>
			</operator>
			<call builtin="+">
				<builtin name="time"/>
				<integer value="2"/>
			</call>
		</equal>
	</equation>

</class>
