mkfile_path := $(abspath $(lastword $(MAKEFILE_LIST)))
mkfile_dir := $(dir $(mkfile_path))
include $(mkfile_dir)/win
BITNESS=64
FORCE_32_BIT=false
EXTRA_SETUP_ARGS=--no-msvcr=true --with_openmp=True

