mkfile_path := $(abspath $(lastword $(MAKEFILE_LIST)))
mkfile_dir := $(dir $(mkfile_path))
include $(mkfile_dir)/win
BITNESS=32
EXTRA_SETUP_ARGS=--force-32bit=true --no-msvcr=true --extra-c-flags=-mincoming-stack-boundary=2

INSTALL_DIR_FOLDER=${SDK_HOME}/install/Python
# Required variables
FMIL_HOME=${SDK_HOME}/build/FMIL_install
FMIL_CONFIG=$(mkfile_dir)/../../FMILibrary/windows/win64
# Optional variables
PYTHONHOME=${SDK_HOME}/Python27/Python_32/
