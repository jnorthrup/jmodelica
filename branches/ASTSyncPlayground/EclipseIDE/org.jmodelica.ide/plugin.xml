<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="org.jmodelica.ide.ModelicaBuilder"
         name="ModelicaBuilder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="false"
            isConfigurable="false">
         <run class="org.jmodelica.ide.ModelicaBuilder"/>
      </builder>
   </extension>
   <extension
         id="org.jmodelica.ide.nature"
         name="Modelica Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.jmodelica.ide.Nature">
         </run>
      </runtime>
      <builder
            id="org.jmodelica.ide.ModelicaBuilder">
      </builder>
   </extension>
   <extension
         id="org.jmodelica.ide.marker.error"
         name="Error"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
   </extension>
   <extension
         id="org.jmodelica.ide.marker.error.syntactic"
         name="Syntax error"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.jmodelica.ide.marker.error">
      </super>
   </extension>
   <extension
         id="org.jmodelica.ide.marker.error.semantic"
         name="Semantic error"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.jmodelica.ide.marker.error">
      </super>
   </extension>
   <extension
         id="org.jmodelica.ide.marker.error.warning"
         name="Warning"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.jmodelica.ide.marker.error.semantic">
      </super>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            default-charset="UTF-8"
            file-extensions="mo"
            id="org.jmodelica.ide.content.modelica"
            name="Modelica File"
            priority="normal">
      </content-type>
      <content-type
            file-extensions="fmu"
            id="org.jmodelica.ide.content.fmu"
            name="Functional Mockup Unit"
            priority="normal">
      </content-type>
   </extension>
   <extension
         point="org.eclipse.ui.ide.projectNatureImages">
      <image
            icon="resources/icons/outline/modelica_project.png"
            id="org.jmodelica.ide.nature.image"
            natureId="org.jmodelica.ide.nature">
      </image>
   </extension>
   <extension
         id="org.jmodelica.ide.compiler"
         name="JModelica Compiler"
         point="org.jastadd.ed.core.compilers">
      <compiler
            class="org.jmodelica.ide.compiler.ModelicaEclipseCompiler">
      </compiler>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.jmodelica.ide.compiler.ModelicaPreferences">
      </initializer>
   </extension>
</plugin>
