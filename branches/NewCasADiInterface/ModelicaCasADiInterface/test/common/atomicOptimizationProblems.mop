type Voltage = Real(quantity="ElectricalPotential", unit="V");
optimization atomicWithVoltageType(objectiveIntegrand=x1, finalTime=10) 
    Voltage x1(start=0, fixed = true);
equation
    der(x1)  = sin(x1) + 0.5;
constraint
    x1 >=  1.00;
end atomicWithVoltageType;

