<project name="ModelicaCompilerOptions-module">
	<!-- Needs to be in a module separate without java files, else they are copied -->
    <property name="ModelicaCompilerOptions.generate-pre" value="org.jmodelica.build.options.OptionsAggregator"/>
    <property name="ModelicaCompilerOptions.package.generated-pre" value="${ast_package}.generated"/>
    
    <property name="ModelicaCompilerOptions.java_replace" value="ast"/>
    <property name="ModelicaCompilerOptions.java_replace.ast.from" value="org.jmodelica.modelica.compiler.generated."/>
    <property name="ModelicaCompilerOptions.java_replace.ast.to" value="${ast_package}.generated."/>
    
</project>
