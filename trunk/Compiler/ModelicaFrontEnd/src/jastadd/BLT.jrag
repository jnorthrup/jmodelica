            if (!isSolvable())
                doPrettyPrintIterationVariables(str);
        }

        public void doPrettyPrintIterationVariables(CodeStream str) {
            boolean first = true;
            for (FVariable var : allLocalVariables()) {
                if (first)
                    str.print(" Iteration variables: ");
                else
                    str.print("                      ");
                first = false;
                str.println(var.name());
            }
        }

