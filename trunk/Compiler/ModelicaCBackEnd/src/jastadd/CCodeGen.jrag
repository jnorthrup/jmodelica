	public void WhenEquationBlock.genBlock_C(int i, boolean inInitial, String indent, PrintStream str) {			
		 // if (guard and !pre(guard)) {
		 // 	_x = _pre_x + 1;
		 // }
		
		// TODO: Add support for else when clauses
		for (FExp g : whenEquation.guards()) {		
			g.genTempVars_C(ASTNode.printer_C, str, indent);
			str.format("%s_guards(%d) = ", indent, g.guardExpInEquationsIndex());
			g.prettyPrint_C(str,"");
			str.print(";\n");
		}
		int j = whenEquation.guards().size();
		str.print("  if(");
		for (FExp g : whenEquation.guards()) {
			if (j>1) {
				str.print("LOG_EXP_OR(");
			}		
			str.print("COND_EXP_EQ(LOG_EXP_AND(_guards(");
			str.print(g.guardExpInEquationsIndex());
			str.print("),LOG_EXP_NOT(_pre_guards(");
			str.print(g.guardExpInEquationsIndex());
			str.print("))),JMI_TRUE,JMI_TRUE,JMI_FALSE)");
			if (j>1) {
				str.print(",\n    ");
			}		
			j--;
		}		
		for (int k = 0;k<whenEquation.guards().size()-1;k++) {
			str.print(")");
		}
		str.print(") {\n");
		for (SimpleEquationBlock b : blocks(0)) {
			b.genBlock_C(i,inInitial,indent + " ",str);
		}
		str.print("  } else {\n");
		for (FVariable fv : activeVariables()) {
			str.print(indent);
			str.print(fv.name_C());
			str.print(" = ");
			str.print("pre");
			str.print(fv.name_C());
			str.print(";\n");
			str.print(indent);
		}
		str.print("}\n");
	}

