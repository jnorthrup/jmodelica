	public void AbstractEquationBlock.genBlockResidualFunction_C(int block_counter, boolean inInit, String indent, PrintStream str) {
		str.print("static int dae_" + (inInit? "init_": "") + "block_"+block_counter+"(jmi_t* jmi, jmi_real_t* x, jmi_real_t* residual, int evaluation_mode) {\n");
		str.print("  jmi_real_t** res = &residual;\n");	
		genVarDecls_C(str, indent); // TODO: wrong indent (but so has rest of func...)
		int var_counter = 0;
		// Nominal values
		str.print("  if (evaluation_mode==JMI_BLOCK_NOMINAL) {\n");
		for (FVariable fv : blockResidualVariables_C()) {
			if(((FRealVariable)fv).nominalAttributeSet()) {
				str.print("    x["+var_counter+"] = ");
				str.print(((FRealVariable)fv).nominalAttribute());
				str.append(";\n");
			} 
			var_counter++;
		}
		
		// Min values
		var_counter = 0;
		str.print("  } else  if (evaluation_mode==JMI_BLOCK_MIN) {\n");
		for (FVariable fv : blockResidualVariables_C()) {
			if(((FRealVariable)fv).minAttributeSet()) {
				str.print("    x["+var_counter+"] = ");
				str.print(((FRealVariable)fv).minAttribute());
				str.append(";\n");
			}
			var_counter++;
		}
		
		// Max values
		var_counter = 0;
		str.print("  } else if (evaluation_mode==JMI_BLOCK_MAX) {\n");
		for (FVariable fv : blockResidualVariables_C()) {
			if(((FRealVariable)fv).maxAttributeSet()) {
				str.print("    x["+var_counter+"] = ");
				str.print(((FRealVariable)fv).maxAttribute());
				str.append(";\n");
			} 
			var_counter++;
		}

		str.print("  } else if (evaluation_mode==JMI_BLOCK_INITIALIZE) {\n");
		var_counter = 0;
		// Loop over all active variables and write their value to x	
		for (FVariable fv : blockResidualVariables_C()) {
			str.print(indent + indent + "x["+var_counter+"] = " + fv.name_C() + ";\n");
			if(((FRealVariable)fv).maxAttributeSet() && ((FRealVariable)fv).minAttributeSet()) {
				str.print(indent + indent + "init_with_bounds(");
				str.print("x["+var_counter+"],");
				str.print(((FRealVariable)fv).minAttribute() + ", "+ ((FRealVariable)fv).maxAttribute());
				str.print(", \"Resetting initial value for " + ((FRealVariable)fv).name() + "\");\n");
			} else if(((FRealVariable)fv).maxAttributeSet()) {
				str.print(indent + indent + "init_with_ubound(");
				str.print("x["+var_counter+"]," + ((FRealVariable)fv).maxAttribute());
				str.print(", \"Resetting initial value for " + ((FRealVariable)fv).name() + "\");\n");
			} else if(((FRealVariable)fv).minAttributeSet()) {
				str.print(indent + indent + "init_with_lbound(");
				str.print("x["+var_counter+"]," + ((FRealVariable)fv).minAttribute());
				str.print(", \"Resetting initial value for " + ((FRealVariable)fv).name() + "\");\n");
			} 

			var_counter++;
		}
		
		// Generate code for Jacobian
		genBlockResidualFunctionJacobian_C(indent, str);
		
		str.print(indent + "} else if (evaluation_mode==JMI_BLOCK_EVALUATE) {\n");
		// Loop over all active variables and write the values in x to the z vector
		var_counter = 0;
		for (FVariable fv : blockResidualVariables_C()) {
			if(((FRealVariable)fv).maxAttributeSet() && ((FRealVariable)fv).minAttributeSet()) {
				str.print(indent + indent + "check_bounds(");
				str.print("x["+var_counter+"],");
				str.print(((FRealVariable)fv).minAttribute() + ", "+ ((FRealVariable)fv).maxAttribute());
				str.print(", \"Out of bounds for variable " + ((FRealVariable)fv).name() + "\");\n");
			} else if(((FRealVariable)fv).maxAttributeSet()) {
				str.print(indent + indent + "check_ubound(");
				str.print("x["+var_counter+"]," + ((FRealVariable)fv).maxAttribute());
				str.print(", \"Out of bounds for variable " + ((FRealVariable)fv).name() + "\");\n");
			} else if(((FRealVariable)fv).minAttributeSet()) {
				str.print(indent + indent + "check_lbound(");
				str.print("x["+var_counter+"]," + ((FRealVariable)fv).minAttribute());
				str.print(", \"Out of bound for variable " + ((FRealVariable)fv).name() + "\");\n");
			} 
			var_counter++;
		}
		var_counter = 0;		

		for (FVariable fv : blockResidualVariables_C()) {
			str.print(indent + indent + fv.name_C() + " = x["+var_counter+"];\n");
			var_counter++;
		}
		
		genBlockResidualFunctionBeforeEqns_C(indent, str, new HashSet<FAbstractEquation>());
		// Write the residual expressions
		int equation_counter = 0;
		for (FAbstractEquation e : blockResidualEquations_C()) {
			e.genResidual_C(equation_counter,indent,str);				
			equation_counter += e.numScalarEquations();
		}
		genBlockResidualFunctionAfterEqns_C(indent, str);
		// Write back
		str.print(indent + "} else if (evaluation_mode == JMI_BLOCK_WRITE_BACK) {\n");
		var_counter = 0;
		// Loop over all active variables and write their value to x	
		for (FVariable fv : blockResidualVariables_C()) {
			str.print(indent + indent + fv.name_C() + " = x[" + var_counter +"];\n");
			var_counter++;
		}

		str.print(indent + "}\n");
		str.print(indent + "return 0;\n}\n\n");
	}
	
