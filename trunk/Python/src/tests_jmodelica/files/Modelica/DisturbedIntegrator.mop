optimization DisturbedIntegrator(objectiveIntegrand = x^2 + (u-w)^2)
    Real x(start = 0, fixed = true);
    input Real u;
    input Real w;
equation
    der(x) = u;
end DisturbedIntegrator;

optimization Integrator(objectiveIntegrand = x^2)
    Real x(start = 0, fixed = true);
    input Real u;
equation
    der(x) = u;
end Integrator;
