package ParEst

  model SecondOrder
    parameter Real w(start=1);
    parameter Real z(start=0.5);
    parameter Real x1_0 = 0;
    parameter Real x2_0 = 0;
    Real u;
    Real x1(start=x1_0, nominal=1.5);
    Real x2(start=x2_0, nominal=1.5);
    Real y=x1;
  
  equation
    der(x1) = -2*w*z*x1 + x2;
    der(x2) = -w^2*x1 + w^2*u;
    u = 1;
  end SecondOrder;

  optimization ParEstCasADi(objective = 1, startTime = 0, finalTime = 15)
    // Create second order system instance,
    // Set parameters w and z free and give 
    // initial guesses
    extends SecondOrder(w(free=true, initialGuess=2, min=0.1),
                        z(free=true, initialGuess=1, min=0.1),
                        x1(fixed=true), x2(fixed=true));
  end ParEstCasADi;
  
end ParEst;
